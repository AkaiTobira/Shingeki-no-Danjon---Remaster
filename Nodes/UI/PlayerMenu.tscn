func new_stat_comparator(stat_label, stat, old_value, current_value):
	var diff = old_value - current_value

	if old_value > current_value: 
		stat_label.push_color( Color(0.6,0.6,0.6) ) 
		stat_label.add_text( str( current_value + get_player_statistic(stat) ).substr(0,4)+ \"(-\" +  str(abs(diff)).substr(0,4) + \")\")
	elif old_value < current_value:
		stat_label.push_color( Color(1,1,0) )
		stat_label.add_text( str( current_value + get_player_statistic(stat) ).substr(0,4)+ \"(+\" +  str(abs(diff)).substr(0,4) + \")\")
	else:
		stat_label.push_color( Color(1,1,1) )
		stat_label.add_text( str( current_value + get_player_statistic(stat) ).substr(0,4) + \"(0)\")
	stat_label.pop()


func update_stats(item, i_instance):
	var equiped_item_instance = PlayerStats.equipment[get_eq_slot(item)]
	var old_item
	if PlayerStats.equipment[get_eq_slot(item)]: old_item = Res.items[PlayerStats.equipment[get_eq_slot(item)].id]
	
	if item.type == \"misc\": 
		$Tabs/Equipment/Stats.visible = false
		return
		
	if item.type == \"consumable\":  #osobna obsługa
		
		var irrelevant = true
		for i in range($Tabs/Equipment/Stats/Stats.get_child_count()):
			var stat_label = $Tabs/Equipment/Stats/Stats.get_child(i)
			if i >= len(i_instance.add_to_stat.keys()): stat_label.visible = false
			else:
				irrelevant = false
				stat_label.clear()
				stat_label.push_color( Color(1,1,1) )
				if i_instance.add_to_stat.keys()[i] == \"res_hp\":
					stat_label.add_text( \"Health points: +\" +  str(i_instance.add_to_stat[\"res_hp\"]))
				if i_instance.add_to_stat.keys()[i] == \"res_mn\":
					stat_label.add_text( \"Mana points: +\" +  str(i_instance.add_to_stat[\"res_mn\"]))
				stat_label.visible = true
		$Tabs/Equipment/Stats.visible = !irrelevant
		return
		
	var stats = []

	for stat in PlayerStats.statistic.keys():
		if equiped_item_instance != null:
			if equiped_item_instance.add_to_stat.has(stat):
				stats.append(stat)
				continue
		if i_instance.add_to_stat.has(stat):
			stats.append(stat)
	
	var irrelevant = true
	for i in range($Tabs/Equipment/Stats/Stats.get_child_count()):
		var stat_label = $Tabs/Equipment/Stats/Stats.get_child(i)

		if i >= len(stats): stat_label.visible = false
		else:
			irrelevant = false
			stat_label.clear()
			add_name_to_label( stat_label, stats[i])
			var old_item_value     = 0 
			if equiped_item_instance != null and equiped_item_instance.add_to_stat.has(stats[i]):
				old_item_value = equiped_item_instance.add_to_stat[stats[i]]
			var current_item_value = 0 if not i_instance.add_to_stat.has(stats[i]) else i_instance.add_to_stat[stats[i]]
			
			new_stat_comparator(stat_label, stats[i], old_item_value, current_item_value)
#			
			stat_label.visible = true
	
	$Tabs/Equipment/Stats.visible = !irrelevant

class CategorySorter:
	static func sort(a, b):
		if a[\"id\"] < b[\"id\"]:
			return true
		elif a[\"id\"] == b[\"id\"]:
			if a[\"material\"] > b[\"material\"]:
				return true
			elif a[\"material\"] == b[\"material\"]:
				if a[\"blessing\"] == \"cursed\" and b[\"blessing\"] != \"cursed\":
					return true
				if a[\"blessing\"] == \"holy\" and b[\"blessing\"] == \"none\":
					return true
		return false

func update_category(whatever = null):
	for i in range(CATEGORIES.size()):
		$Tabs/Equipment/Categories.get_child(i).texture = load(\"res://Sprites/UI/Menu/Equipment/EquipmentCategory\" + (\"Active\" if inventory_category == i else \"\") + \".png\")
	
	inventory_list.clear()
	var item_types = [\"consumable\", \"weapon\", \"armor\", \"shield\", \"helmet\", \"boots\", \"pants\", \"ring\", \"amulet\", \"misc\"]
	if inventory_category > 0:
		item_types = CATEGORY_TYPES[CATEGORIES[inventory_category]]
	
	for item in PlayerStats.inventory:
		if item_types.has(Res.items[item.id].type):
			inventory_list.append(item)
	
	inventory_list.sort_custom(CategorySorter,\"sort\")
	
	update_inventory()
	select_inventory()

func refresh():
	crafting_select = 0
	crafting_scroll = 0
	inventory_select = 0
	self.inventory_scroll = 0
	
	update_statistics()
	update_equipment()
	update_category()
	update_crafting()
	
	$Tabs/Crafting/Scrollbar/ActualScroll.rect_position.y = 0
	$Tabs/Crafting/Scrollbar.visible = (crafting_list.size() > 8)
	$Tabs/Equipment/Inventory/Scrollbar/ActualScrollbar.rect_position.y = 0
	$Tabs/Equipment/Inventory/Scrollbar.visible = (inventory_list.size() > 8)

func scroll_inventory(scroll):
	inventory_scroll = scroll
	var max_scroll = inventory_list.size()-7
	$Tabs/Equipment/Inventory/Scrollbar/ActualScrollbar.rect_position.y = inventory_scroll / max(float(max_scroll), 1) * 283

func auto_move(delta):
	if !auto_move or auto_move[1] == \"up\" and !Input.is_action_pressed(\"ui_up\") or auto_move[1] == \"down\" and !Input.is_action_pressed(\"ui_down\"):
		auto_move = null
		return 0
	
	auto_move[0] -= delta
	if auto_move[0] <= 0:
		if auto_move[1] == \"up\": return -1
		elif auto_move[1] == \"down\": return 1
	return 0"
