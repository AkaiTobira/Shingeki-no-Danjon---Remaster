			get_parent().get_node(\"Sprite2\").scale = Vector2(0.15, 0.19)
		4:
			get_parent().get_node(\"Shape3\").disabled = false
			get_parent().get_node(\"Sprite2\").scale = Vector2(0.15, 0.19)


