script/source = "extends Node2D

func set_player_position(none):
	Res.game.player.position = $Position2D.position

var chests      = []
var initialized = false
var blocked     = []

func initialize():
	if initialized : return 
	
	chests = []
	for i in range(16): 
		var chest = load( \"res://Nodes/Objects/Chest.tscn\" ).instance()
		
		var used_range =  $BottomTiles.get_used_rect().size 
		
		var positon = Vector2( randi()%( int(used_range[0]) - 1 ) + $BottomTiles.get_used_rect().position[0] + 1,
							   randi()%( int(used_range[1]) - 3 ) + $BottomTiles.get_used_rect().position[1] + 3 ) * 80
		
		while positon in blocked: 
			positon = Vector2( randi()%( int(used_range[0]) - 1 ) + $BottomTiles.get_used_rect().position[0] + 1,
							   randi()%( int(used_range[1]) - 3 ) + $BottomTiles.get_used_rect().position[1] + 3 ) * 80
		
		chest.position = positon
		blocked.append(positon)
		chests.append(chest)
	initialized = true

func fill(): 
	for chest in chests:
		chest.initialize(0,\"Mechanic\", \"Chest\", 8)
		add_child( chest )

func clear(): 
	for child in get_children():
		if \"hest\" in child.name: 
			call_deferred( \"remove_child\", child )
			call_deferred( \"queue_free\", child )

func _process(delta): pass"

